<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name="com.example.dcache.SampleApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/Theme.DoraCacheSample"
        tools:targetApi="n">
        <activity android:name="com.example.dcache.cover.CoverActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.dcache.http.CrawlerActivity"
            android:label="@string/cover_api_request"
            android:exported="true"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.example.dcache.orm.BasicDataTypeTestActivity"
            android:label="@string/cover_data_storage"
            android:exported="true"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.example.dcache.orm.ComplexDataTypeTestActivity"
            android:label="@string/cover_data_storage"
            android:exported="true"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.example.dcache.orm.OrmWriteTestActivity"
            android:label="@string/cover_data_storage"
            android:exported="true"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.example.dcache.orm.OrmReadTestActivity"
            android:label="@string/cover_data_storage"
            android:exported="true"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.example.dcache.orm.OrmTestCasesActivity"
            android:label="@string/cover_data_storage"
            android:exported="true"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.example.dcache.orm.OrmModelEditorActivity"
            android:label="@string/cover_data_storage"
            android:exported="true"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.example.dcache.cache.CacheActivity"
            android:label="@string/cover_data_cache"
            android:exported="true"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.example.dcache.tutorial.TutorialActivity"
            android:label="@string/cover_sunshine"
            android:exported="true"
            android:screenOrientation="portrait"/>
    </application>

</manifest>